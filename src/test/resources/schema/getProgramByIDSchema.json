{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "programId": {
      "type": "integer"
    },
    "programName": {
      "type": "string"
    },
    "programDescription": {
      "type": ["string", "null"]
    },
    "programStatus": {
      "type": "string"
    },
    "creationTime": {
      "type": "string",
      "format": "date-time"
    },
    "lastModTime": {
      "type": "string",
      "format": "date-time"
    }
  },
  "required": ["programId", "programName", "programStatus", "creationTime", "lastModTime"]
}
